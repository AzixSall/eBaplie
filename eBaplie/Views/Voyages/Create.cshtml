@model eBaplie.Models.Voyage

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Voyage</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="VoygeNumber" class="control-label"></label>
                <input asp-for="VoygeNumber" class="form-control" />
                <span asp-validation-for="VoygeNumber" class="text-danger"></span>
            </div>
            <div class="form-group" hidden>
                <label asp-for="VesselId" class="control-label"></label>
                <input asp-for="VesselId" class="form-control" value="@ViewBag.VesselId" />
            </div>
            <div class="form-group">
                <label asp-for="BaplieFile" class="control-label"></label>
                <input name="BaplieFile" class="form-control" type="file" />
                <span asp-validation-for="BaplieFile" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
